#, fuzzy, no-wrap
msgid "Setting fonts for xterm"
msgstr "xtermのフォントを設定"

#. type: Content of: <guide><chapter><section><body><pre>
#: en/guide-localization.xml:515
#, fuzzy, no-wrap
msgid ""
"<comment>(in your home directory)</comment>\n"
"$ <i>echo 'XTerm*font: fixed' >> .Xresources </i>\n"
"$ <i>xrdb -merge .Xresources</i>\n"
msgstr ""
"<comment>(ホームディレクトリで)</comment>\n"
"$ <i>echo 'XTerm*font: fixed' >> .Xresources </i>\n"
"$ <i>xrdb -merge .Xresources</i>\n"

